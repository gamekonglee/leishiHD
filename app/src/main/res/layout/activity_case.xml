<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#f3f3f3"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#f3f3f3"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/x45"
    android:background="@color/theme_dark_top">
    <ImageView
        android:onClick="goBack"
        android:background="@mipmap/nav_icon_back"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/x30"
        android:layout_width="@dimen/x20"
        android:layout_height="@dimen/x20" />
    <TextView
        android:text="实景案例"
        android:textColor="#ffffff"
        android:textSize="@dimen/x16"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
    <LinearLayout
        android:background="#ffffff"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x47"
        android:gravity="center"
        android:orientation="vertical">
        <astuetz.MyPagerSlidingTabStrip
            android:id="@+id/tab_strip"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/x45"/>
        <androidx.viewpager.widget.ViewPager
            android:id="@+id/vp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"/>
    </LinearLayout>
    <GridView
        android:id="@+id/gv_case"
        android:numColumns="3"
        android:horizontalSpacing="@dimen/x15"
        android:verticalSpacing="@dimen/x15"
        android:paddingRight="@dimen/x30"
        android:paddingLeft="@dimen/x30"
        android:paddingBottom="@dimen/x15"
        android:paddingTop="@dimen/x15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>

<ImageView
android:onClick="goBack"
android:background="@mipmap/icon_return"
android:layout_width="@dimen/x50"
android:layout_height="@dimen/x50"
android:layout_alignParentRight="true"
android:layout_alignParentBottom="true"
android:layout_margin="@dimen/x30"/>
</RelativeLayout>